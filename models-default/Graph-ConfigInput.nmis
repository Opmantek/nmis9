%hash = (
	'heading' => 'UPS Config Input Detail',
	'title' => {
		'standard' => '$node - $length from $datestamp_start to $datestamp_end',
		'short' => 'UPS Config Input'
	},
	'vlabel' => {
		'standard' => 'UPS Config Input'
	},
	'option' => {
		'standard' => [
			'DEF:upsConfigInputV=$database:upsConfigInputV:AVERAGE',
