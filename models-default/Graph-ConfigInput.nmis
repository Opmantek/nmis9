			'GPRINT:upsConfigInputV:MIN:Min %1.2lf',
			'GPRINT:upsConfigInputV:MAX:Max %1.2lf\\n',

			'DEF:upsConfigInputFreq=$database:upsConfigInputFreq:AVERAGE',
			'LINE2:upsConfigInputFreq#ff0000:UPS Config Input Freq 0.1 Hertz',
			'GPRINT:upsConfigInputFreq:LAST:Current %1.2lf',
