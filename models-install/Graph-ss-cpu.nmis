%hash = (
	'title' => {
		'standard' => '$node - $length from $datestamp_start to $datestamp_end',
		'short' => '$node - $length'
	},
	'vlabel' => {
		'standard' => 'CPU Usage by Type'
	},
	'option' => {
		'standard' => [
			'--lower-limit','0',
			'DEF:ssCpuRawUser=$database:ssCpuRawUser:AVERAGE',
			'DEF:ssCpuRawNice=$database:ssCpuRawNice:AVERAGE',
			'DEF:ssCpuRawSystem=$database:ssCpuRawSystem:AVERAGE',
			'DEF:ssCpuRawIdle=$database:ssCpuRawIdle:AVERAGE',
			'DEF:ssCpuRawWait=$database:ssCpuRawWait:AVERAGE',
			'DEF:ssCpuRawKernel=$database:ssCpuRawKernel:AVERAGE',
			'DEF:ssCpuRawInterrupt=$database:ssCpuRawInterrupt:AVERAGE',
			'DEF:ssCpuRawSoftIRQ=$database:ssCpuRawSoftIRQ:AVERAGE',

			'AREA:ssCpuRawUser#ff0000:User CPU\\t',
			'GPRINT:ssCpuRawUser:AVERAGE:Avg %6.2lf\\t',
			'STACK:ssCpuRawSystem#ffa500:System CPU\\t',
			'GPRINT:ssCpuRawSystem:AVERAGE:Avg %6.2lf\\t',
			'STACK:ssCpuRawWait#ffff00:Wait IO CPU\\t',
			'GPRINT:ssCpuRawWait:AVERAGE:Avg %6.2lf\\n',
			'STACK:ssCpuRawKernel#00ffff:Kernel CPU\\t',
			'GPRINT:ssCpuRawKernel:AVERAGE:Avg %6.2lf\\t',
			'STACK:ssCpuRawNice#ff00ff:Nice CPU\\t',
			'GPRINT:ssCpuRawNice:AVERAGE:Avg %6.2lf\\t',
			'STACK:ssCpuRawInterrupt#0000ff:Interupt CPU\\t',
			'GPRINT:ssCpuRawInterrupt:AVERAGE:Avg %6.2lf\\n',
			'STACK:ssCpuRawSoftIRQ#00a5ff:SoftIRQ CPU\\t',
			'GPRINT:ssCpuRawSoftIRQ:AVERAGE:Avg %6.2lf\\t',
			'STACK:ssCpuRawIdle#00ff00:Idle CPU\\t',
			'GPRINT:ssCpuRawIdle:AVERAGE:Avg %6.2lf\\n',
		]
	}
);








