	DisplayString, DateAndTime
		FROM SNMPv2-TC
	MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
		FROM SNMPv2-CONF;
     
opmantek MODULE-IDENTITY
	LAST-UPDATED "201503160000Z"
	ORGANIZATION "Opmantek Ltd"
	CONTACT-INFO
		"Opmantek Ltd
		Level 13
		50 Cavill Avenue
		Surfers Paradise, Queensland 4217
		Australia
		Tel: 
		Web: https://community.opmantek.com
		"
	DESCRIPTION
		"Opmantek root assignements
		This module defines the Opmantek SNMP MIB root
		and its primary subtrees.
		"

	REVISION      "201503160000Z"
	DESCRIPTION
		"The initial revision."
	::= { enterprises 4818 }     

omkNotifications OBJECT-IDENTITY
	STATUS  current
	DESCRIPTION
		"Parent of all Opmantek notifications (traps, informs)."
	::= { opmantek 1 }

omkObjects OBJECT-IDENTITY
	STATUS  current
	DESCRIPTION
		"Parent of all Opmantek notifications (traps, informs)."
	::= { opmantek 2 }

omkEvent NOTIFICATION-TYPE
	OBJECTS     { omkId, omkTime, omkDate, omkNode, omkHost, omkEvent, omkElement, omkState, omkStateful, omkDetails, omkType, omkPriority, omkLevel }
	STATUS      current
	DESCRIPTION 
		"The Opmantek Event coming from opEvents with rich data about state and priority."
	::= { omkNotifications 1 }

nmisEvent NOTIFICATION-TYPE
	OBJECTS     { nmisTime, nmisNode, nmisEvent, nmisLevel, nmisElement, nmisDetails }
	STATUS      current
	DESCRIPTION 
		"The NMIS Event with standard event properties."
	::= { omkNotifications 2 }


omkEventGroup OBJECT-GROUP
	OBJECTS {
		omkId,
		omkTime,
		omkDate,
		omkNode,
		omkHost,
		omkEvent,
		omkElement,
		omkState,
		omkStateful,
		omkDetails,
		omkType,
		omkPriority,
		omkLevel
	} 
	STATUS current
	DESCRIPTION
		"These objects make up the Event details in the OMK notification."
	::= { omkObjects 1 }

omkId OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION 
		"A globally unique Event ID"
	::= { omkEventGroup 1 }

omkTime OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION 
		"Unix time of the event (seconds since 1970)"
	::= { omkEventGroup 2 }

omkDate OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION 
		"The event time in human readable format"
	::= { omkEventGroup 3 }

omkNode OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION 
