	::= { omkEventGroup 10 }

omkType OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION 
		"Where did the event originate?"
	::= { omkEventGroup 11 }

omkPriority OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION 
		"opEvents priority level, see opEvents priority levels vs. NMIS and Syslog levels"
	::= { omkEventGroup 12 }

omkLevel OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION 
